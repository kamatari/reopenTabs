{
	"manifest_version":2,
	"name":"Reopen Tabs",
	"description":"reopen closed tabs",
	"version":"2.0",
	"background": {
		"scripts" :	["reopenTabs.js"]
	},
	"permissions":[
		"tabs"
	],
	"icons": {
		"16": "icon16.png",
		"128": "icon128.png"
	},
	"browser_action":{
		"default_icon"	: "reopenTabs.png",
		"default_title"	: "reopenTabs"
	}
}
